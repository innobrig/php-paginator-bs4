{
    "name": "innobrig/paginator",
    "type": "library",
    "description": "A lightweight PHP paginator for Bootstrap4. Generates pagination controls in the style of Stack Overflow and Flickr. The 'first' and 'last' page links are shown inline as page numbers, and excess page numbers are replaced by ellipses.",
    "keywords": ["paginator", "pagination", "pager", "Bootstrap4"],
    "homepage": "http://github.com/innobrig/php-paginator-bs4",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Grimes",
            "email": "jason@grimesit.com"
        },
        {
            "name": "Innovation Brigade",
            "email": "contact@innovation-brigade.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-0": {
            "InnoBrig\\Paginator\\": "src/"
        },
        "psr-4": {
            "InnoBrig\\Paginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {"InnoBrig\\Paginator\\Tests": "tests/"}
    }

}


